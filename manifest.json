{
  "manifest_version": 2,
  "name": "replay sans pub",
  "version": "0.1",
  "description": "Retirer la pub en replay",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": [
        "https://www.tf1.fr/*",
        "http://www.tf1.fr/*"
      ],
      "js": ["app.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
